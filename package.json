{
  "name": "image-gallery",
  "version": "0.0.1",
  "description": "Image Gallery Web Component",
  "homepage": "https://github.com/newtonmunene99/mpesa-api/",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@stencil/core": "^0.9.1",
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "@types/jest": "^21.1.1",
    "jest": "^21.2.1"
  },
  "author": {
    "name": "Newton Munene",
    "email": "newtonmunene99@gmail.com"
  },
  "license": "MIT",
  "keywords": [
    "Web Components",
    "Image Gallery",
    "Stencil",
    "Masonry CSS"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newtonmunene99/mpesa-api.git"
  },
  "bugs": {
    "url": "https://github.com/newtonmunene99/mpesa-api/issues"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
